#Imprima el directorio de trabajo actual 
pwd
#Crear un directorio test1
mkdir test1
#Crear el archivo test1.txt
touch test1/test1.txt
#Guardar el texto Hola GBI6 en un archivo test2.txt
touch test1/test2.txt
nano test1/test2.txt
 Hola GBI6
#Guardar la instrucción print("Hola GBI6")  en el archivo test1.py
touch test1/test1.py
nano test1/test1.py
 print("Hola GBI6")
